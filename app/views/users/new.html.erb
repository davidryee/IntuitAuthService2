<h1>Create User</h1>


<%= form_with scope: :user, url: users_path, local: true do |form| %>
    <p>
      <%= form.label :username %><br>
      <%= form.text_field :username %>
    </p>

    <p>
      <%= form.label :password %><br>
      <%= form.text_area :password %>
    </p>

    <p>
      <%= form.submit %>
    </p>
<% end %>